{
  "n回繰り返す": {
    "prefix": "narr",
    "body": ["[...Array(${1:n}).keys()]${2:.map((i) => i + 1)}"],
    "description": "配列作る"
  },
  "for": {
    "prefix": "for",
    "body": [
			"for (let ${1:i} = ${2:0}; ${1:i} ${3:<} ${4:n}; ${1:i}++) {",
			"\t$5",
			"}"
		],
    "description": "for"
  }
}
