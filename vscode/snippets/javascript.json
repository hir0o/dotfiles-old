  "atoms": {
    "prefix": "ima",
    "body": [
      "import $1 from '~/components/atoms/$1'"
    ],
    "description": "コンポーネントインポートするやつ"
  },
  "molecules": {
    "prefix": "imm",
    "body": [
      "import $1 from '~/components/molecules/$1'"
    ],
    "description": "コンポーネントインポートするやつ"
  },
  "プロップス": {
