{
  "editor.snippetSuggestions": "top", // ユーザースニペットを既存のスニペットより上に出す
  "window.zoomLevel": 1,
  "todo-tree.tree.showScanModeButton": false,
  "workbench.iconTheme": "material-icon-theme",
  "todo-tree.highlights.enabled": false,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.lastDownload": 1603449952973,
  "files.autoSave": "onFocusChange",
  "editor.semanticTokenColorCustomizations": null,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.includeLanguages": {
    "erb": "html",
    "scss": "css"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk-11.0.2.jdk/Contents/Home",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "java.semanticHighlighting.enabled": true,
  "csscomb.preset": "csscomb",
  "files.associations": {
    "*.vue": "vue",
    "*.gs": "javascript"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.detectIndentation": false,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "cSpell.userWords": ["Tollero"],
  "files.associations": {
    "*.gs": "javascript"
  },
  "tabnine.experimentalAutoImports": true,
  "go.alternateTools": {
    "go-langserver": "gopls"
  },
  "go.buildOnSave": "package",
  "go.formatTool": "goimports",
  "go.formatFlags": ["-w"],
  "go.gocodeAutoBuild": true,
  "go.installDependenciesWhenBuilding": true,
  "go.languageServerExperimentalFeatures": {
    "hover": true,
    "format": true,
    "rename": true,
    "diagnostics": true,
    "autoComplete": false,
    "signatureHelp": true,
    "goToDefinition": true,
    "findReferences": true,
    "documentSymbols": true,
    "workspaceSymbols": true,
    "goToTypeDefinition": true,
    "goToImplementation": true
  },
  "go.lintTool": "golint",
  "go.lintOnSave": "package",
  "gopls": {
    "deepCompletion": true,
    "usePlaceholders": false,
    "completeUnimported": true
  },
  "go.testOnSave": false,
  "go.useLanguageServer": true,
  "go.vetOnSave": "package",
  "workbench.colorTheme": "Ayu Dark Bordered",
  "liveServer.settings.donotVerifyTags": true,
  "java.refactor.renameFromFileExplorer": "autoApply"
}
